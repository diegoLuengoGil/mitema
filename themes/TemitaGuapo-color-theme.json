{
  "name": "TemitaGuapo",
  "colors": {
    "editor.background": "#fffb00",
    "editor.foreground": "#ffffff",
    "activityBar.background": "#ff0000", // Fondo de la barra de actividades (iconos verticales)
    "activityBar.foreground": "#ff00cc", // Color de los iconos
    "activityBarBadge.background": "#ff00cc",
    "activityBarBadge.foreground": "#0f0022",
    "sideBar.background": "#ff7300", // Fondo de la barra lateral
    "sideBar.foreground": "#00fff7", // Texto de la barra lateral
    "titleBar.activeBackground": "#09ff00", // Fondo de la barra de arriba activa
    "titleBar.activeForeground": "#ffffff", // Texto de la barra de arriba activa
    "titleBar.inactiveBackground": "#d9ff00", // Fondo de la barra de arriba inactiva
    "titleBar.inactiveForeground": "#888888", // Texto de la barra de arriba inactiva
    // Personalización de la barra de pestañas
    "editorGroupHeader.tabsBackground": "#2f00ff", // Fondo de la barra de pestañas
    "tab.activeBackground": "#2d0055", // Fondo de la pestaña activa

    "tab.inactiveBackground": "#1a0033", // Fondo de las pestañas inactivas

    "tab.border": "#ff00cc",
    "sideBarTitle.foreground": "#00fff7",
    // ...existing code...
    "tab.unfocusedActiveForeground": "#fffb00", // Texto y "x" de pestaña activa pero no enfocada
    "tab.unfocusedInactiveForeground": "#ff00cc", // Texto y "x" de pestaña inactiva pero no enfocada
    "tab.activeForeground": "#fffb00",
    "tab.inactiveForeground": "#ff00cc" // <-- Cambia el color de la "x" en pestañas inactivas
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00ff00"
      }
    },
    // ...existing code...
    {
      "name": "Variables",
      "scope": ["variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    // Añade este nuevo bloque en la sección "tokenColors":
    {
      "name": "Variables principales (Python)",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite.python",
        "variable.other.assignment"
      ],
      "settings": {
        "foreground": "#ff0000" // ROJO
      }
    },
    {
      "name": "Class, Support",
      "scope": ["entity.name.type.class", "support.class", "support.type"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    // ...existing code...
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#680000"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
